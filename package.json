{
  "name": "@whinc/web-console",
  "version": "0.3.0-beta.4",
  "author": "whincwu@163.com",
  "license": "MIT",
  "keywords": [
    "console",
    "vConsole",
    "web-console",
    "webConsole",
    "log"
  ],
  "repository": "https://github.com/whinc/web-console",
  "main": "dist/web-console.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name web-console --dest dist src/main.js",
    "build:web": "vue-cli-service build --target app --dest demo",
    "predepoly": "npm run build:web",
    "depoly": "npx gh-pages -d demo",
    "postversion": "npm run build && npm login && npm publish --access=public && echo publish ${npm_package_version} success!!!",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md && git commit -m 'docs(changelog): 更新' || echo 'changelog 没变化'",
    "report": "vue-cli-service build --report",
    "test": "vue-cli-service test",
    "test:api": "cd ./tests/api && nodemon index.js",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint src/**/*",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "cookie_js": "^1.2.2",
    "highlight.js": "^9.12.0",
    "mint-ui": "^2.2.13",
    "url-search-params": "^1.0.2",
    "vue": "^2.5.17",
    "vue-infinite-scroll": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.4",
    "babel-plugin-component": "^1.1.1",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^1.2.0",
    "husky": "^1.0.0-rc.13",
    "i": "^0.3.6",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "prettier": "1.14.2",
    "pretty-quick": "^1.6.0",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
