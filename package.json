{
  "name": "@whinc/web-console",
  "version": "0.1.5",
  "author": "whincwu@163.com",
  "license": "MIT",
  "keywords": [
    "console",
    "vConsole",
    "web-console",
    "webConsole",
    "log"
  ],
  "repository": "https://github.com/whinc/web-console",
  "main": "dist/web-console.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name web-console src/main.js --dest dist",
    "build:web": "vue-cli-service build --dest demo",
    "predepoly": "npm run build:web",
    "depoly": "npx gh-pages -d demo",
    "test": "vue-cli-service test",
    "testServer": "cd ./server && node index.js",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "husky": "^1.0.0-rc.13",
    "mint-ui": "^2.2.13",
    "pretty-quick": "^1.6.0",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.8",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-rc.8",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.8",
    "@vue/cli-plugin-unit-jest": "^3.0.0-rc.8",
    "@vue/cli-service": "^3.0.0-rc.8",
    "@vue/test-utils": "^1.0.0-beta.21",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.4",
    "babel-plugin-component": "^1.1.1",
    "gh-pages": "^1.2.0",
    "i": "^0.3.6",
    "node-sass": "^4.9.2",
    "prettier": "1.14.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
